<body>
    <div id="Moscow map" style="width: 600px; height: 400px"></div>
</body>
<script>
    ymaps.ready(init);
    var myMap;

    function init(){     
        myMap = new ymaps.Map ("Moscow map", {
            center: [55.76, 37.64],
            zoom: 12
        });
        myMap.controls.add(
				   new ymaps.control.SmallZoomControl()
				);
    }
</script>